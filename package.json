{
  "name": "hellpack",
  "version": "3.0.0",
  "engines": {
    "node": ">=10.18.0",
    "yarn": ">=1.22.0"
  },
  "bin": {
    "hellpack": "bin/hellpack"
  },
  "files": [
    "bin"
  ],
  "description": "From 0 to eslint, real quick!",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajatsharma/hellpack.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap && cargo install",
    "build": "cargo build",
    "dev": "cargo run",
    "release": "cargo build --release",
    "wrap": "yarn release && cp target/release/hellpack bin/"
  },
  "bugs": {
    "url": "https://github.com/rajatsharma/hellpack/issues"
  },
  "homepage": "https://github.com/rajatsharma/hellpack#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "lerna": "^3.5.1"
  }
}
